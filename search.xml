    <url>/2021/10/31/%E8%BE%93%E5%85%A5%E4%B8%80%E8%A1%8C%E5%AD%97%E7%AC%A6%EF%BC%8C%E5%88%86%E5%88%AB%E7%BB%9F%E8%AE%A1%E5%87%BA%E5%85%B6%E4%B8%AD%E8%8B%B1%E6%96%87%E5%AD%97%E6%AF%8D-%E7%A9%BA%E6%A0%BC-%E6%95%B0%E5%AD%97%E5%92%8C%E5%85%B6%E4%BB%96%E5%AD%97%E7%AC%A6%E7%9A%84%E4%B8%AA%E6%95%B0/</url>
    <content><![CDATA[<figure class="highlight plaintext"><table><tr><td class="code"><pre><span class="line">#include &lt;stdio.h&gt;</span><br><span class="line">int main()</span><br><span class="line">&#123;</span><br><span class="line"> char c;</span><br><span class="line"> int letters=0,space=0,digit=0,other=0;</span><br><span class="line"> printf(&quot;请输入一行字符：&quot;);</span><br><span class="line"> while ((c=getchar())!=&#x27;\n&#x27;)&#123;</span><br><span class="line">  if (c&gt;=&#x27;a&#x27;&amp;&amp;c&lt;=&#x27;z&#x27;||c&gt;=&#x27;A&#x27;&amp;&amp;c&lt;=&#x27;Z&#x27;)</span><br><span class="line">   letters++;</span><br><span class="line">  else if (c==&#x27; &#x27;)</span><br><span class="line">   space++;</span><br><span class="line">  else if (c&gt;=&#x27;0&#x27;&amp;&amp;c&lt;=&#x27;9&#x27;)</span><br><span class="line">   digit++;</span><br><span class="line">  else </span><br><span class="line">   other++;</span><br><span class="line"> &#125;</span><br><span class="line"> printf(&quot;字母数:%d\n空格数:%d\n数字数:%d\n其他字符:%d\n&quot;,letters,space,digit,other);</span><br><span class="line"> return 0;</span><br><span class="line">&#125;</span><br></pre></td></tr></table></figure>]]></content>
